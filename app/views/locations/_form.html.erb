<%= simple_form_for @location, class: "bg-light", remote: true do |f| %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :studio %>
  <%= f.input :region %>
  <%= f.input :country, :as => :hidden, :input_html => { :value => session[:country] } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
