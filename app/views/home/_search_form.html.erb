<%= simple_form_for "", url: search_path do |f| %>
  <%= f.input :keyword %>
  <%= f.input :teacher %>
  <%= f.input :class_type %>
  <%= f.input :day_of_week %>
  <%= f.input :time_after %>
  <%= f.input :time_before %>
  <%= f.input :duration %>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :state %>
  <%= f.input :studio %>
  <%= f.input :day_after %>
  <%= f.input :day_before %>


  <%= f.submit "Search", class: "btn btn-outline-primary" %>
<% end %>
